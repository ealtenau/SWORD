# SWORD Database Configuration
# Copy to .env and fill in your values

# Primary backend: 'duckdb' or 'postgres'
SWORD_PRIMARY_BACKEND=duckdb

# PostgreSQL connection (when using postgres backend)
SWORD_POSTGRES_URL=postgresql://user:password@localhost:5432/sword_v17c

# DuckDB path (when using duckdb backend)
SWORD_DUCKDB_PATH=data/duckdb/sword_v17c.duckdb
